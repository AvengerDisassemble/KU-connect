{
  "name": "ku-connect-backend",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "Avengers Disassemble",
  "type": "commonjs",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db jest --runInBand --forceExit",
    "test:quiet": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db jest --silent --runInBand --forceExit",
    "test:verbose": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db SHOW_LOGS=true jest --runInBand",
    "test:summary": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db jest --verbose=false --runInBand",
    "test:failures": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db jest --onlyFailures --verbose=false --runInBand",
    "test:setup": "cross-env NODE_ENV=test DATABASE_URL=file:./test.db npx prisma migrate deploy && node prisma/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@prisma/client": "^6.16.3",
    "bcrypt": "^6.0.0",
    "chai": "^6.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.3.2",
    "install": "^0.13.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "npm": "^11.6.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "uuid": "^13.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
